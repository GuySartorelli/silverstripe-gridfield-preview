{
    "name": "guysartorelli/silverstripe-gridfield-preview",
    "type": "silverstripe-vendormodule",
    "description": "Standalone silverstripe GridField component for previewing records directly from the GridField",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "gridfield",
        "preview"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.11",
        "silverstripe/admin": "^1.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "GuySartorelli\\GridFieldPreview\\": "src/"
        }
    }
}
